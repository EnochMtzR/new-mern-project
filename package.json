{
  "name": "inhotel-web",
  "version": "1.0.0",
  "description": "Integra's InHotel version for web",
  "main": "index.js",
  "scripts": {
    "serve": "ts-node server/main",
    "build": "webpack --config webpack.dev.js -w",
    "create-folders": "mkdir -p release/public/dist && cp -r public/fonts/ release/public/",
    "set:release": "npm run create-folders && cp key.pem server.crt release/ && cp public/index.html release/public/",
    "build-server:prod": "tsc -p tsconfig_Server-prod.json",
    "build-client:prod": "webpack --config webpack.prod.js",
    "build:prod": "npm run set:release && npm run build-server:prod && npm run build-client:prod",
    "test": "NODE_ENV=test jest --config=jest.config.js && live-server coverage/lcov-report/",
    "test-u": "NODE_ENV=test jest --updateSnapshot --config=jest.config.js && live-server coverage/lcov-report/"
  },
  "keywords": [
    "pms"
  ],
  "author": "Jose Enoc Martinez Rojas",
  "license": "GPL-3.0-or-later",
  "dependencies": {
